{
  "name": "playwright-dom-extractor",
  "version": "1.0.0",
  "description": "Extract DOM elements from web pages, generate semantic keys, and create Playwright tests",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "build": "tsc",
    "monitor-page": "node --loader ts-node/esm dom-monitor-cli.ts",
    "scan-specs": "node --loader ts-node/esm scan-spec-files.ts",
    "update-semantics": "node --loader ts-node/esm update-semantic-index.ts",
    "basic-demo": "node examples/dom-extraction-demo.js",
    "mcp-demo": "node examples/mcp-integration-demo.js",
    "semantic-demo": "playwright test examples/essential-demo.spec.ts"
  },
  "keywords": [
    "playwright",
    "dom",
    "extraction",
    "testing",
    "automation",
    "semantic"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@playwright/test": "^1.40.1",
    "chalk": "^4.1.2",
    "commander": "^10.0.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.0",
    "inquirer": "^9.2.7",
    "open": "^9.1.0",
    "ora": "^6.3.1",
    "playwright": "^1.40.1",
    "playwright-core": "^1.40.1",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
